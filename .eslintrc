{
  "env": {
    "browser": true,
    "node": true,
    "amd": false,
    "mocha": false,
    "jasmine": false
  },
  "globals": {
    "angular": true,
    "_": true,
    "moment": true,
    "$": true,
    "jQuery": true
  },
  "rules": {
    "no-use-before-define": [
      1,
      "nofunc"
    ],
    "quotes": [
      0,
      "single"
    ],
    "camelcase": 1,
    "semi": 0,
    "no-undef": 0,
    "yoda": [
      0,
      "never"
    ],
    "eqeqeq": 1,
    "strict": 0,
    "eol-last": 1,
    "no-trailing-spaces": 1,
    "no-shadow": 1,
    "comma-spacing": 1,
    "key-spacing": 1,
    "comma-dangle": 1,
    "no-cond-assign": 1,
    "no-console": 1,
    "no-constant-condition": 2,
    "no-control-regex": 1,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty-character-class": 1,
    "no-empty": 1,
    "no-ex-assign": 1,
    "no-extra-boolean-cast": 1,
    "no-extra-parens": [
      1,
      "all"
    ],
    "no-extra-semi": 1,
    "no-func-assign": 2,
    "no-inner-declarations": 1,
    "no-invalid-regexp": 1,
    "no-irregular-whitespace": 1,
    "no-negated-in-lhs": 1,
    "no-obj-calls": 1,
    "no-regex-spaces": 1,
    "no-sparse-arrays": 1,
    "no-unreachable": 1,
    "use-isnan": 1,
    "valid-typeof": 2,
    "no-fallthrough": 2,
    "no-octal": 2,
    "no-redeclare": 2,
    "no-delete-var": 1,
    "no-unused-vars": 1,
    "array-bracket-spacing": 1,
    "arrow-parens": 1,
    "arrow-spacing": 1,
    "generator-star-spacing": 1,
    "no-const-assign": 2,
    "no-var": 1,
    "object-shorthand": 1,
    "prefer-const": 1,
    "prefer-spread": 1,
    "prefer-reflect": 1,
    "require-yield": 2
  }
}
